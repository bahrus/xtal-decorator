// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.js.map" : true
    },
    "html.experimental.customData": ["./custom-elements.json"],
    "json.schemas": [
        {
            "fileMatch": ["/custom-elements.json"],
            "url": "https://unpkg.com/wc-info@0.0.64/schema.json"
        },
        {
            "fileMatch": ["/css.json"],
            "url": "https://raw.githubusercontent.com/Microsoft/vscode-css-languageservice/master/docs/customData.schema.json"
        }
    ]
}